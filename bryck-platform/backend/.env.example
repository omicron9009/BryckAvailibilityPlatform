# Application
APP_ENV=development
APP_NAME=Bryck Availability Platform
APP_VERSION=1.0.0
DEBUG=true

# Database
DATABASE_URL=postgresql+asyncpg://bryck_user:bryck_pass@localhost:5432/bryck_db

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://127.0.0.1:5500

# Mock BryckAPI base URL (replace with real URL in production)
BRYCK_API_BASE_URL=http://mock-bryckapi:9000

# Pagination defaults
DEFAULT_PAGE_SIZE=50
MAX_PAGE_SIZE=200
